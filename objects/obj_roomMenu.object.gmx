            global.dateStableVersion = ini_read_string("StableVersion","dateStableVersion","SERVER ERROR")
            global.currentTestVersion = ini_read_string("TestVersion","currentTestVersion","SERVER ERROR")
            global.dateTestVersion = ini_read_string("TestVersion","dateTestVersion","SERVER ERROR")
        ini_close();

if os_is_network_connected() {
    if showInitialPanel = 0 &amp;&amp; checkVersion = 1{
        if (string(global.currentStableVersion) != string(gameVersion)) {
             showUpdate = 1;
        }
    }
} else {
    showUpdate = 0;
}

if showUpdate = 1 {
    draw_set_color(c_black);
    draw_set_font(fnt_default12);
    draw_set_halign(fa_center);
    draw_set_valign(fa_center);
    draw_text(x,y-20,"There is an update available to download #(" + string(currentStableVersion) + ", uploaded in " + string(dateStableVersion) + ")##Would you like to download it?");
    
    draw_set_color(c_maroon);
    draw_text(x,y+130,"Yes (enter)      No (esc)")
    
    if keyboard_check_pressed(vk_enter) {
        url_open("http://www.triunitystudios.com/en/update")
        game_end();
    }
    if keyboard_check_pressed(vk_escape) {
        showUpdate = 0;
        checkVersion = 0;
    }

}
