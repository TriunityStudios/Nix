        <arguments>
          <argument>
            <kind>1</kind>
            <string>    // Inputs
    key_right = keyboard_check(ord("D"));
    key_left = -keyboard_check(ord("A"));
    key_jump = keyboard_check_pressed(vk_space);
    keep_key_jump = keyboard_check(vk_space);
    globalvar player_x; player_x = x;
    globalvar player_y; player_y = y;
    
    // diring left and right
    if(key_right = 1 &amp;&amp; place_free(x+2,y) &amp;&amp; key_left = 0){
                x+=2;   // "x+=3" is the same than "x=x+3"
                sprite_index = spr_player_runR;
                image_speed = .4;
                dir = 1;
        } 
         
    if (key_left = 1 &amp;&amp; place_free(x-2,y) &amp;&amp; key_right = 0) {
                x-=2;
                sprite_index = spr_player_runL;
                image_speed = .4; 
                dir = -1;     
    }
      
    
    // Gravity
    if (place_free(x,y+1)) {
        vspeed += 0.35;
        if (vspeed &gt; 0){
        fall = 1;
        }

        // Set max fall speed
        if (vspeed &gt;= 10) {
        vspeed = 10;
        }
     } 
     
        // Bug fix "be in walls"
    if (!place_free(x,y)){
        y-=1;
    }
    
        // Test Bug fix "fly"
    if (vspeed &lt; -10) {
        vspeed = -10;
    }
        
    // Jump
    if(key_jump = 1 &amp;&amp; !place_free(x,y+1)){
        vspeed = -6;
    } 
     
    // Climbing 1/2 Block
    if (place_free(x+3, y-10) &amp;&amp; !place_free(x+3, y) &amp;&amp; keep_key_jump = 1 &amp;&amp; key_right = 1){
            sprite_index = spr_e_soldierR;
            vspeed = 0;
            y-=0.36;
            climb1 = 1
    }
    if (place_free(x-3, y-10) &amp;&amp; !place_free(x-3, y) &amp;&amp; keep_key_jump = 1 &amp;&amp; key_left = 1){
            sprite_index = spr_e_soldierL;
            vspeed = 0;
            y-=0.36;
            climb1 = 1
    }


</string>
